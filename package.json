{
  "name": "chroma-sass",
  "version": "1.2.4",
  "description": "Chroma is the Sass color manager, managing color names, variations, and color schemes.",
  "homepage": "https://github.com/JohnAlbin/chroma",
  "bugs": {
    "url": "https://github.com/JohnAlbin/chroma/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/JohnAlbin/chroma.git"
  },
  "author": "John Albin Wilkins <virtually.johnalbin@gmail.com> (http://john.albin.net/)",
  "keywords": [
    "eyeglass-module",
    "sass",
    "colors"
  ],
  "main": "sass/_chroma.scss",
  "style": "sass/_chroma.scss",
  "eyeglass": {
    "sassDir": "sass",
    "exports": false,
    "name": "chroma",
    "needs": "*"
  },
  "directories": {
    "lib": "sass",
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "posttest": "eslint .",
    "docs": "kss-node --source sass --source docs --destination gh-pages --title 'Chroma for Sass'"
  },
  "license": "GPL-2.0",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.0.0",
    "eyeglass": "^0.8.3",
    "kss": "3.0.0-beta.3",
    "mocha": "^2.4.5",
    "sassy-test": "^3.0.4"
  }
}
